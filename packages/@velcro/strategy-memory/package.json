{
  "name": "@velcro/strategy-memory",
  "version": "0.29.1",
  "description": "Velcro strategy for resolving files from an in-memory object",
  "keywords": [
    "velcro",
    "resolve",
    "npm",
    "resolution",
    "module",
    "browser",
    "unpkg"
  ],
  "author": "Geoff Goodman <ggoodman@gmail.com>",
  "homepage": "https://github.com/ggoodman/velcro",
  "license": "MIT",
  "browser": "./dist/dist-browser/index.js",
  "main": "./dist/dist-main/index.js",
  "module": "./dist/dist-module/index.js",
  "unpkg": "./dist/dist-unpkg/index.min.js",
  "types": "./dist/dist-main/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ggoodman/velcro.git"
  },
  "scripts": {
    "build": "rollup -c",
    "build:api": "api-extractor run --local -c ./api-extractor.json",
    "build:readme": "cat README.md | sed \"s~tree/.*/docs~tree/$(jq -r .version package.json)/docs~g\" > README.md",
    "prepack": "npm run build"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.8.1",
    "@microsoft/api-extractor": "^7.8.1",
    "@rollup/plugin-alias": "^3.1.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@wessberg/rollup-plugin-ts": "^1.2.24",
    "rollup": "^2.10.0",
    "rollup-plugin-inject-process-env": "^1.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@velcro/common": "^0.29.1",
    "@velcro/resolver": "^0.29.1",
    "ts-primitives": "^2.4.0"
  },
  "gitHead": "4a44dfc05c89d85f127e1eb9d34ff6690e285bb2"
}
